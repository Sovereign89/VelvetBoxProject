<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="125.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.geekbrains.velvetbox.controller.ConfirmController">
   <children>
      <Button fx:id="YesButton" layoutX="14.0" layoutY="85.0" mnemonicParsing="false" onAction="#YesButtonAction" prefHeight="25.0" prefWidth="138.0" text="Да">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="NoButton" cancelButton="true" defaultButton="true" layoutX="168.0" layoutY="85.0" mnemonicParsing="false" onAction="#NoButtonAction" prefHeight="30.0" prefWidth="138.0" text="Нет">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="60.0" prefWidth="292.0" text="Вы уверены, что хотите перенести сюда выбранный файл?" textAlignment="JUSTIFY" wrapText="true">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
