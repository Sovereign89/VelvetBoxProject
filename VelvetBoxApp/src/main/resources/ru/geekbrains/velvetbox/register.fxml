<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="160.0" prefWidth="330.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.geekbrains.velvetbox.controller.RegisterController">
   <children>
      <TextField fx:id="NameField" layoutX="77.0" layoutY="14.0" onKeyPressed="#UsernameOnKeyPressed" prefHeight="25.0" prefWidth="243.0" promptText="Введите ФИО" />
      <TextField fx:id="LoginField" layoutX="77.0" layoutY="50.0" onKeyPressed="#LoginOnKeyPressed" prefHeight="25.0" prefWidth="243.0" promptText="Введите логин" />
      <PasswordField fx:id="PasswordField" layoutX="77.0" layoutY="89.0" onKeyPressed="#PasswordOnKeyPressed" prefHeight="25.0" prefWidth="243.0" promptText="Введите пароль" />
      <Button fx:id="RegisterButton" layoutX="14.0" layoutY="128.0" mnemonicParsing="false" onAction="#RegisterButtonAction" onKeyPressed="#RegisterOnKeyPressed" prefHeight="25.0" prefWidth="156.0" text="Зарегистрироваться" />
      <Button fx:id="CancelButton" layoutX="177.0" layoutY="128.0" mnemonicParsing="false" onAction="#CancelButtonAction" onKeyPressed="#CancelOnKeyPressed" prefHeight="25.0" prefWidth="143.0" text="Отмена" />
      <Label alignment="CENTER_RIGHT" layoutX="14.0" layoutY="18.0" prefHeight="17.0" prefWidth="61.0" text="ФИО:" />
      <Label alignment="CENTER_RIGHT" layoutX="14.0" layoutY="54.0" prefHeight="17.0" prefWidth="61.0" text="Логин:" />
      <Label alignment="CENTER_RIGHT" layoutX="14.0" layoutY="93.0" prefHeight="17.0" prefWidth="61.0" text="Пароль:" />
   </children>
</AnchorPane>
